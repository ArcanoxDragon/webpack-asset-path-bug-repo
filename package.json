{
	"name": "my-test-project",
	"sideEffects": false,
	"version": "1.0.0",
	"description": "Test project",
	"author": "Me",
	"type": "module",
	"private": true,
	"scripts": {
		"clean": "NODE_OPTIONS=\"--import tsx\" rimraf node_modules/.cache .eslintcache",
		"rebuild": "yarn run clean && NODE_OPTIONS=\"--import tsx\" webpack --progress",
		"build": "NODE_OPTIONS=\"--import tsx\" webpack --progress",
		"build-fast": "NODE_OPTIONS=\"--import tsx\" webpack --progress --env skipTypeCheck --env skipLinting",
		"watch": "NODE_OPTIONS=\"--import tsx\" webpack --progress --watch",
		"prod": "NODE_OPTIONS=\"--import tsx\" webpack --progress --env prod",
		"prod-fast": "NODE_OPTIONS=\"--import tsx\" webpack --progress --env prod --env skipTypeCheck --env skipLinting",
		"profile": "NODE_OPTIONS=\"--import tsx\" webpack --progress --env prod --profile --json >stats.json",
		"analyze": "NODE_OPTIONS=\"--import tsx\" webpack --progress --env stats",
		"lint": "NODE_OPTIONS=\"--import tsx\" eslint src --cache"
	},
	"devDependencies": {
		"@eamodio/eslint-lite-webpack-plugin": "^0.2.0",
		"@eslint/js": "^9.20.0",
		"@stylistic/eslint-plugin": "^4.0.1",
		"@swc/core": "1.10.15",
		"@swc/plugin-emotion": "^8.6.0",
		"@types/node": "^22.13.4",
		"@types/webpack-bundle-analyzer": "^4.7.0",
		"@typescript-eslint/eslint-plugin": "^8.24.1",
		"@typescript-eslint/parser": "^8.24.1",
		"browserslist": "^4.24.4",
		"chalk": "^5.4.1",
		"css-loader": "^7.1.2",
		"eslint": "^9.20.1",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^5.1.0",
		"fork-ts-checker-webpack-plugin": "^9.0.2",
		"globals": "^16.0.0",
		"globby": "^14.1.0",
		"node-polyfill-webpack-plugin": "^4.1.0",
		"rimraf": "^6.0.1",
		"sass-embedded": "^1.85.0",
		"sass-loader": "^16.0.5",
		"style-loader": "^4.0.0",
		"swc-loader": "^0.2.6",
		"swc-minify-webpack-plugin": "^2.1.3",
		"tsconfig-paths-webpack-plugin": "^4.2.0",
		"tsx": "^4.19.3",
		"typescript": "^5.7.3",
		"webpack": "5.98.0",
		"webpack-bundle-analyzer": "^4.10.2",
		"webpack-cli": "^6.0.1"
	},
	"packageManager": "yarn@4.6.0"
}
